<%= render "layouts/errors", object: @listing %>

<%= form_for @listing do |f| %>
  <%= f.label :location %>
  <%= f.text_field :location, class: 'form-control', required: true, autofocus: true %>
  <%= f.label :price %>
  <%= f.number_field :price, class: 'form-control', required: true %>
  <%= f.label :property_type %>
  <%= f.text_field :property_type, size: "30x6", class: 'form-control', required: true %>
  <br>
  <%= f.submit class: 'btn btn-success', method: "post" %>
<% end %>
